{
    "name": "ziggy-js",
    "version": "2.0.0-beta.1",
    "description": "Use your Laravel named routes in JavaScript.",
    "keywords": [
        "laravel",
        "routes",
        "ziggy"
    ],
    "homepage": "https://github.com/tighten/ziggy",
    "bugs": "https://github.com/tighten/ziggy/issues",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/tighten/ziggy.git"
    },
    "authors": [
        {
            "name": "Daniel Coulbourne",
            "email": "daniel@tighten.co"
        },
        {
            "name": "Jake Bathman",
            "email": "jake@tighten.co"
        },
        {
            "name": "Jacob Baker-Kretzmar",
            "email": "jacob@tighten.co"
        }
    ],
    "files": [
        "src/js",
        "dist"
    ],
    "source": "src/js/index.js",
    "main": "dist/ziggy.umd.js",
    "module": "dist/ziggy.module.js",
    "exports": "dist/ziggy.modern.js",
    "scripts": {
        "build": "microbundle --format modern,es,umd --external none --no-sourcemap",
        "watch": "npm run build watch",
        "build:npm": "microbundle --format modern,es,umd --no-sourcemap",
        "test": "jest --verbose",
        "prepublishOnly": "npm run build:npm"
    },
    "mangle": {
        "regex": "^_(?!query)"
    },
    "dependencies": {
        "qs": "~6.9.0"
    },
    "devDependencies": {
        "babel-preset-power-assert": "^3.0.0",
        "jest": "^27.0.6",
        "microbundle": "^0.13.3",
        "power-assert": "^1.6.1"
    }
}
